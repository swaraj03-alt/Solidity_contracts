pragma solidity ^0.8.19;
contract _datatype
{
    string  public name ;
    uint256  public Number;
    int256 public a = 1223;
    int256 public d; // 00
    bool public b; //by default false
    bool  public c = true;
    address public addr;
    int public minInt = type(int).min;
    int public maxInt = type(int).max;

}
